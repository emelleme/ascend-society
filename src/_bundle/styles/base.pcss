.hero {
  background-color: #f8f8f8;
  text-align: center;
  padding: 50px 0;
}



.hero img {
  max-width: 100%;
  height: auto;
}

.hero-content {
  margin-top: 30px;
  display: flex;
  flex-direction: column;
}

.hero-content h1 {
  font-size: 32px;
  color: #333;
  margin-bottom: 20px;
}

.second-text {
  font-size: 24px;
  color: #666;
