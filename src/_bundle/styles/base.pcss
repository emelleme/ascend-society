    }
}


.title-hero {
    color: white;
    font-size: 36px;
    width: 500px;    
    background-color: rgba(22,11,0,0.46);
    border-width: 2px;
    border-color: #f89f37;
    padding: 40px; 
    font-family: 'Raleway', sans-serif;
    position: absolute;
    bottom: 20px;
    left: 50%;
    transform: translate(-50%);
      @media screen and (max-width: 600px) {
       font-size: 18px;
       width: 200px;
       padding: 20px;

    }
}

.title-img {
    position: relative;
    min-width: 600px;
       @media screen and (max-width: 1050px) {
      min-width: unset;

    }
    img {
        min-width: 600px;
         @media screen and (max-width: 1050px) {
        width: 100%;
        min-width: unset;

    }
    }
}


