    }
}

.cta-buttons {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  margin-top: 20px;
}

.cta-buttons .btn {
  display: inline-flex;
  align-items: center;
  margin: 5px;
  padding: 10px 20px;
  background-color: #333;
  color: #fff;
  text-decoration: none;
  border-radius: 4px;
    @media screen and (max-width: 600px) {
